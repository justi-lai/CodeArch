{
  "name": "codearch",
  "displayName": "CodeArch",
  "description": "",
  "version": "1.1.0",
  "author": "Justin Lai",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/justi-lai/CodeArch.git"
  },
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "extensionDependencies": [
    "vscode.git"
  ],
  "contributes": {
    "commands": [
      {
        "command": "codearch.codearchAnalyze",
        "title": "CodeArch: Analyze"
      },
      {
        "command": "codearch.configureAI",
        "title": "CodeArch: Configure AI Provider"
      },
      {
        "command": "codearch.setApiKey",
        "title": "CodeArch: Set AI API Key"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "codearch.codearchAnalyze",
          "group": "modification",
          "when": "editorHasSelection"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "fetch-parsers": "./download-parsers.sh"
  },
  "devDependencies": {
    "@types/emscripten": "^1.41.5",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.104.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  },
  "dependencies": {
    "web-tree-sitter": "^0.20.8"
  }
}